{
  "name": "ctest",
  "displayName": "ctest",
  "description": "a simple block of testing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "snippets": [
      {
